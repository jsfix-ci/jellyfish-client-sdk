{
  "name": "@balena/jellyfish-client-sdk",
  "version": "7.0.11",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-client-sdk.git"
  },
  "engines": {
    "node": ">=12.15.0"
  },
  "description": "HTTP client SDK for Jellyfish",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && npm run check && deplint && depcheck --ignore-bin-package --ignores=@types/jest",
    "lint:fix": "balena-lint --fix lib test",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "check": "jellycheck"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^0.24.0",
    "bluebird": "^3.7.2",
    "common-tags": "^1.8.2",
    "deep-copy": "^1.4.2",
    "fast-json-patch": "^3.1.0",
    "is-uuid": "^1.0.2",
    "lodash": "^4.17.21",
    "socket.io-client": "^4.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@balena/jellycheck": "^1.0.0",
    "@balena/jellyfish-config": "^1.5.10",
    "@balena/jellyfish-types": "^1.2.21",
    "@balena/lint": "^6.1.1",
    "@types/bluebird": "^3.5.36",
    "@types/common-tags": "^1.8.1",
    "@types/is-uuid": "^1.0.0",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.177",
    "@types/sinon": "^10.0.6",
    "@types/socket.io-client": "^3.0.0",
    "@types/uuid": "^8.3.3",
    "depcheck": "^1.4.2",
    "deplint": "^1.1.3",
    "jest": "^27.0.6",
    "lint-staged": "^12.1.1",
    "nock": "^13.2.1",
    "simple-git-hooks": "^2.7.0",
    "sinon": "^12.0.1",
    "ts-jest": "^27.0.3",
    "typedoc": "^0.22.9",
    "typescript": "^4.3.5"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2021-11-22T04:48:32.422Z"
  }
}
